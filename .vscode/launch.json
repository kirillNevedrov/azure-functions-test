{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "justMyCode": false,
      "type": "python",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      },
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Attach to Python Functions",
      "type": "python",
      "request": "attach",
      "port": 9091,
      "preLaunchTask": "func: host start"
    }
  ]
}
